<% if !session[:logged_user].blank? %>
	<%=render :partial => "logout"%>
<% end %>
<% if session[:logged_user] %>
	<% if User.find_by_username(session[:logged_user]).has_role?("superadmin") %>
		<p class="message">Logged as superadmin</p>
		<%= button_to "Users", users_path, method: :get %>
	<% elsif User.find_by_username(session[:logged_user]).has_role?("admin") %>
		<p class="message">Logged as admin</p>
	<% else %>
		<p class="message">Logged as user</p>
	<% end %>
<% end %>
